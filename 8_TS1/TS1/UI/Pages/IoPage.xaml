<UserControl x:Class="Ipte.UI.Pages.IoPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:gcl="clr-namespace:Ipte.TS1.UI.Controls;assembly=Ipte.TS1.UI.Controls"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    Width="980" Height="550" >
    
    <UserControl.Resources>
    </UserControl.Resources>
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <TextBlock x:Uid="TextBlock_Inputs" Text="Inputs" Margin="16,8,0,0" FontSize="12" FontWeight="Bold" />
        <TextBlock x:Uid="TextBlock_Outputs" Text="Outputs" Margin="16,8,0,0" Grid.Column="1" FontSize="12" FontWeight="Bold" />
        <gcl:GuiIoControl Margin="16,24,8,16" Name="Inputs" Grid.Column="0"/>
        <gcl:GuiIoControl Margin="16,24,8,16" Name="Outputs" Grid.Column="1"/>
    </Grid>
</UserControl>
